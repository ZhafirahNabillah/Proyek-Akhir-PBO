<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" prefHeight="372.0" prefWidth="600.0" style="-fx-background-color: fe4068;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.LoginController">
    <children>
        <AnchorPane layoutX="276.0" layoutY="-1.0" prefHeight="404.0" prefWidth="47.0" style="-fx-background-color: #ff9999;" />
        <AnchorPane layoutX="241.0" layoutY="-1.0" prefHeight="404.0" prefWidth="47.0" style="-fx-background-color: #ffcccc;" />
        <AnchorPane prefHeight="402.0" prefWidth="47.0" style="-fx-background-color: #ff9999;" />
        <AnchorPane layoutX="30.0" layoutY="49.0" prefHeight="353.0" prefWidth="47.0" style="-fx-background-color: #ffcccc;" />
        <ImageView fitHeight="93.0" fitWidth="173.0" layoutX="-5.0" layoutY="-14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Gambar/PPG_logo.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="248.0" fitWidth="285.0" layoutX="21.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Gambar/powerpuff-girl-png-3.png" />
            </image>
        </ImageView>
        <AnchorPane layoutX="323.0" layoutY="-1.0" prefHeight="404.0" prefWidth="277.0" style="-fx-background-color: #ffcccc;">
            <children>
                <Label layoutX="32.0" layoutY="23.0" text="Power Puff Girls" textFill="#fe4680">
                    <font>
                        <Font name="Showcard Gothic" size="23.0" />
                    </font>
                </Label>
                <AnchorPane layoutX="14.0" layoutY="63.0" prefHeight="321.0" prefWidth="249.0" style="-fx-background-color: #ff9999; -fx-background-radius: 20;">
                    <children>
                        <Button fx:id="btnLogin" layoutX="98.0" layoutY="137.0" mnemonicParsing="false" onAction="#LogInClick" style="-fx-background-color: #fe4680; -fx-background-radius: 20;" text="Masuk" textFill="WHITE">
                            <font>
                                <Font name="Square721 BT Roman" size="12.0" />
                            </font>
                        </Button>
                        <TextField fx:id="username" layoutX="26.0" layoutY="39.0" prefHeight="23.0" prefWidth="192.0" promptText="username/email">
                            <font>
                                <Font name="Square721 BT Roman" size="12.0" />
                            </font>
                        </TextField>
                        <PasswordField fx:id="password" layoutX="26.0" layoutY="78.0" prefHeight="22.0" prefWidth="192.0" promptText="password" />
                        <Label fx:id="account" layoutX="36.0" layoutY="180.0" text="Anda belum memiliki akun ?" textFill="WHITE">
                            <font>
                                <Font name="Serif Regular" size="10.0" />
                            </font>
                        </Label>
                        <Button fx:id="btnRegister" layoutX="157.0" layoutY="175.0" mnemonicParsing="false" onAction="#RegisterClick" style="-fx-background-color: #ffff; -fx-background-radius: 10;" text="Daftar" textFill="#7a0606">
                            <font>
                                <Font name="Square721 BT Roman" size="11.0" />
                            </font>
                        </Button>
                  <Button fx:id="btnAdmin" layoutX="26.0" layoutY="266.0" mnemonicParsing="false" onAction="#clickBtnAdmin" style="-fx-background-color: #fe4680; -fx-background-radius: 10;" text="Admin" textFill="WHITE">
                     <font>
                        <Font name="Square721 BT Roman" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnPsikolog" layoutX="166.0" layoutY="266.0" mnemonicParsing="false" onAction="#clickBtnPsikolog" style="-fx-background-color: #fe4680; -fx-background-radius: 10;" text="Psikolog" textFill="WHITE" />
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
